<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:urn="wso2.connector.ebay">
   <soapenv:Header></soapenv:Header>
   <soapenv:Body>
        <urn:apiUrl>%s(shoppingApiUrl)</urn:apiUrl>
        <urn:appId>%s(appId)</urn:appId>
        <urn:siteId>%s(siteId)</urn:siteId>
        <urn:messageId></urn:messageId>
        <urn:version>829</urn:version>
        <urn:productSort>%s(productSort)</urn:productSort>
        <urn:availableItemsOnly>%s(availableItemOnly)</urn:availableItemsOnly>
        <urn:queryKeywords>%s(query)</urn:queryKeywords>
        <urn:method>findProducts</urn:method>
    </soapenv:Body>
</soapenv:Envelope>