<!--The sequence will removes all the unwanted response headers from TradeGecko responses.-->
<sequence xmlns="http://ws.apache.org/ns/synapse" name="tradegecko-removeResponseHeaders">
	<!--Remove unwanted Headers from the TradeGecko response-->
	<header name="Etag" scope="transport" action="remove"/>
	<header name="Vary" scope="transport" action="remove"/>
	<header name="Content-Encoding" scope="transport" action="remove"/>
</sequence>