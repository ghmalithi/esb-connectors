<?xml version="1.0" encoding="UTF-8"?>
<template name="removeGroupFromProjectRole" xmlns="http://ws.apache.org/ns/synapse">
    <parameter description="a String containing the name of the group" name="groupName"/>
    <parameter description="a String containing the id of the project" name="projectId"/>
    <parameter description="a String containing the id of the role" name="roleId"/>
    <sequence>
        <payloadFactory media-type="xml">
            <format>
                <soapenv:Envelope
                    xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/"
                    xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
                    <soapenv:Body>
                        <ns1:removeActorsFromProjectRole
                            soapenv:encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" xmlns:ns1="http://soap.rpc.jira.atlassian.com">
                            <in0 xsi:type="xsd:string">?</in0>
                            <in1 soapenc:arrayType="xsd:string[1]"
                                xmlns:soapenc="http://schemas.xmlsoap.org/soap/encoding/" xsi:type="soapenc:Array">
                                <in1 xsi:type="xsd:string">$1</in1>
                            </in1>
                            <in2 href="#id0"/>
                            <in3 href="#id1"/>
                            <in4 xsi:type="xsd:string">atlassian-group-role-actor</in4>
                        </ns1:removeActorsFromProjectRole>
                        <multiRef id="id1" soapenc:root="0"
                            soapenv:encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"
                            xmlns:ns2="http://beans.soap.rpc.jira.atlassian.com"
                            xmlns:soapenc="http://schemas.xmlsoap.org/soap/encoding/" xsi:type="ns2:RemoteProject">
                            <id xsi:type="xsd:string">$2</id>
                        </multiRef>
                        <multiRef id="id0" soapenc:root="0"
                            soapenv:encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"
                            xmlns:ns3="http://beans.soap.rpc.jira.atlassian.com"
                            xmlns:soapenc="http://schemas.xmlsoap.org/soap/encoding/" xsi:type="ns3:RemoteProjectRole">
                            <id href="#id2"/>
                        </multiRef>
                        <multiRef id="id2" soapenc:root="0"
                            soapenv:encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"
                            xmlns:soapenc="http://schemas.xmlsoap.org/soap/encoding/" xsi:type="xsd:long">$3</multiRef>
                    </soapenv:Body>
                </soapenv:Envelope>
            </format>
            <args>
                <arg evaluator="xml" expression="$func:groupName"/>
                <arg evaluator="xml" expression="$func:projectId"/>
                <arg evaluator="xml" expression="$func:roleId"/>
            </args>
        </payloadFactory>
        <property action="remove" name="Accept-Encoding" scope="transport"/>
        <property name="SOAPAction" scope="transport" value="urn:mediate"/>
        <property name="messageType" scope="axis2" value="text/xml"/>
        <property name="POST_TO_URI" scope="axis2" type="STRING" value="true"/>
        <log level="full"/>
        <call>
            <endpoint>
                <http format="soap11" method="POST" uri-template="{uri.var.jira.url}/rpc/soap/jirasoapservice-v2"/>
            </endpoint>
        </call>
        <property action="remove" name="NO_ENTITY_BODY" scope="axis2"/>
    </sequence>
</template>
