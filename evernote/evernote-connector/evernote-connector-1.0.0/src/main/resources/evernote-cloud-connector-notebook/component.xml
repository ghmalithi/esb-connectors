<component name="evernote-cloud-connector-notebook" type="synapse/template">

    <subComponents>
        <component name="createNotebook">
            <file>createNotebook.xml</file>
            <description>Create a Notebook</description>
        </component>
        <component name="expungeNotebook">
            <file>expungeNotebook.xml</file>
            <description>Permanently removes an existing Notebook</description>
        </component>
        <component name="getNotebook">
            <file>getNotebook.xml</file>
            <description>Retrieves the state of a single Notebook</description>
        </component>
        <component name="listNotebooks">
            <file>listNotebooks.xml</file>
            <description>Retrieves all the Notebooks</description>
        </component>
        <component name="updateNotebook">
            <file>updateNotebook.xml</file>
            <description>update the state of a single Notebooks</description>
        </component>
        <component name="createSharedNotebook">
            <file>createSharedNotebook.xml</file>
            <description>Create a shared notebook</description>
        </component>
        <component name="updateSharedNotebook">
            <file>updateSharedNotebook.xml</file>
            <description>Update a shared notebook</description>
        </component>
    </subComponents>

</component>
