<component name="evernote-cloud-connector-tag" type="synapse/template">

    <subComponents>
        <!--Tag-->
        <component name="createTag">
            <file>createTag.xml</file>
            <description>Makes a new Tag in the account</description>
        </component>
        <component name="expungeTag">
            <file>expungeTag.xml</file>
            <description>Permanently removes an existing Tag</description>
        </component>
        <component name="getTag">
            <file>getTag.xml</file>
            <description>Removes a Tag from any Notes</description>
        </component>
        <component name="unTagAll">
            <file>unTagAll.xml</file>
            <description>Removes a Tag from any Notes</description>
        </component>
        <component name="listTags">
            <file>listTags.xml</file>
            <description>Returns a list of all of the Tags in the account</description>
        </component>
        <component name="updateTag">
            <file>updateTag.xml</file>
            <description>Changes an existing Tag</description>
        </component>
    </subComponents>

</component>
